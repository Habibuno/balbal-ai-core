{
	"name": "balbal-io",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9.0.0"
	},
	"engineStrict": true,
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"preview": "vite preview",
		"release": "standard-version",
		"release:alpha": "standard-version --prerelease alpha",
		"release:beta": "standard-version --prerelease beta",
		"release:rc": "standard-version --prerelease rc",
		"prepare": "husky install",
		"lint-staged": "lint-staged",
		"security:audit": "npm audit",
		"security:fix": "npm audit fix",
		"security:force-fix": "npm audit fix --force",
		"security:snyk": "snyk test",
		"security:snyk-fix": "snyk wizard",
		"security:check": "npm run security:audit && npm run security:snyk",
		"quality": "npm run lint && npm run format:check",
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false",
		"typecheck:strict": "tsc --noEmit --strict --noImplicitAny --noImplicitThis --strictNullChecks --strictFunctionTypes --strictBindCallApply --strictPropertyInitialization --noImplicitReturns --noFallthroughCasesInSwitch --noUncheckedIndexedAccess --noImplicitOverride --noPropertyAccessFromIndexSignature --exactOptionalPropertyTypes --noUnusedLocals --noUnusedParameters"
	},
	"dependencies": {
		"@babel/generator": "^7.24.1",
		"@babel/parser": "^7.24.1",
		"@babel/traverse": "^7.24.1",
		"@babel/types": "^7.24.0",
		"@codemirror/basic-setup": "^0.20.0",
		"@codemirror/lang-css": "^6.3.1",
		"@codemirror/lang-html": "^6.4.9",
		"@codemirror/lang-javascript": "^6.2.3",
		"@codemirror/lang-markdown": "^6.2.2",
		"@codemirror/theme-one-dark": "^6.1.2",
		"@codesandbox/sandpack-react": "2.20.0",
		"@codesandbox/sandpack-themes": "2.0.21",
		"@headlessui/react": "^1.7.18",
		"@stackblitz/sdk": "1.11.0",
		"@uiw/react-codemirror": "^4.21.24",
		"clsx": "^2.1.0",
		"dotenv": "^16.4.5",
		"i18next": "^23.10.1",
		"i18next-browser-languagedetector": "^7.2.0",
		"lucide-react": "^0.344.0",
		"process": "^0.11.10",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-i18next": "^14.1.0",
		"react-native": "^0.73.4",
		"react-native-web": "^0.19.10",
		"react-router-dom": "^6.22.3",
		"rollup-plugin-visualizer": "6.0.3",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-conventional": "^18.6.2",
		"@eslint/js": "^9.9.1",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"@vitejs/plugin-react": "^4.3.1",
		"@whoa-studio/eslint-config": "0.0.14",
		"@whoa-studio/prettier-biome-config": "0.0.32",
		"autoprefixer": "^10.4.18",
		"conventional-changelog-conventionalcommits": "^6.1.0",
		"eslint": "^9.9.1",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.11",
		"globals": "^15.9.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"snyk": "^1.1297.1",
		"standard-version": "^9.5.0",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.3.0",
		"vite": "^5.4.2"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	},
	"standard-version": {
		"header": "# Changelog\n\nAll notable changes to this project will be documented in this file.",
		"types": [
			{
				"type": "feat",
				"section": "‚ú® Features"
			},
			{
				"type": "fix",
				"section": "üêõ Bug Fixes"
			},
			{
				"type": "docs",
				"section": "üìö Documentation"
			},
			{
				"type": "style",
				"section": "üíé Styles"
			},
			{
				"type": "refactor",
				"section": "‚ôªÔ∏è Code Refactoring"
			},
			{
				"type": "perf",
				"section": "‚ö° Performance Improvements"
			},
			{
				"type": "test",
				"section": "‚úÖ Tests"
			},
			{
				"type": "build",
				"section": "üë∑‚Äç Builds"
			},
			{
				"type": "ci",
				"section": "üîß Continuous Integration"
			},
			{
				"type": "chore",
				"section": "üì¶ Chores"
			},
			{
				"type": "revert",
				"section": "‚è™ Reverts"
			}
		]
	},
	"overrides": {
		"glob-parent": "^6.0.2",
		"postcss": "^8.4.35",
		"semver": "^7.5.4"
	},
	"resolutions": {
		"glob-parent": "^6.0.2",
		"postcss": "^8.4.35",
		"semver": "^7.5.4"
	},
	"packageManager": "npm@9.9.2",
	"publishConfig": {
		"access": "restricted"
	},
	"security": {
		"audit": {
			"level": "high",
			"ignore": []
		},
		"allowedLicenses": [
			"MIT",
			"Apache-2.0",
			"BSD-2-Clause",
			"BSD-3-Clause",
			"ISC"
		],
		"vulnerabilityThreshold": {
			"high": 0,
			"medium": 5,
			"low": 10
		}
	}
}
